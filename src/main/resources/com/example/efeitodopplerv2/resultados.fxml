<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.efeitodopplerv2.ResultadosController">
    <children>
        <Label fx:id="lblResultados" layoutX="20.0" layoutY="20.0" text="Retorna as variáveis geradas pelo software" />
        <VBox layoutX="20.0" layoutY="60.0" spacing="20.0">
            <Label fx:id="lblResultado" text="Resultado:"/>
            <!-- Adicione aqui os campos que exibem os resultados da simulação -->
        </VBox>
        <Button fx:id="btnNovaSimulacao" layoutX="700.0" layoutY="280.0" mnemonicParsing="false" onAction="#onNovaSimulacaoButtonClick" text="Nova Simulação" />
        <Button fx:id="btnOuvirAudioGerado" layoutX="700.0" layoutY="320.0" mnemonicParsing="false" onAction="#onOuvirAudioButtonClick" text="Ouvir o áudio gerado" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</AnchorPane>
